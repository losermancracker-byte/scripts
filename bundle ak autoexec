
local scriptUrl = "https://raw.githubusercontent.com/losermancracker-byte/scripts/refs/heads/main/bundle%20ak%20hexlust"

local scriptCode = game:HttpGet(scriptUrl)
loadstring(scriptCode)()

if queue_on_teleport then
    queue_on_teleport([[
        task.wait(6)   -- or wait(6) if your exploit still prefers the old name
        loadstring(game:HttpGet("]] .. scriptUrl .. [["))()
    ]])
end
