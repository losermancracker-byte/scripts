    vu3 = vu2.Character or vu2.CharacterAdded:Wait()
    vu5 = vu3:WaitForChild("Humanoid")
    vu7 = vu3:WaitForChild("HumanoidRootPart")
    vu5.AnimationPlayed:Connect(vu63)
end
vu64()
