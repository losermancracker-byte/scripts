local function vu40()
    if vu10 then
        vu10:Destroy()
    end
    if vu11 then
        vu11:Destroy()
    end
    if vu12 then
        vu12:Disconnect()
    end
    if vu13 then
        vu13:Destroy()
    end
    local v35 = vu7
    local v36, v37, v38 = ipairs(v35:GetChildren())
    while true do
        local v39
        v38, v39 = v36(v37, v38)
        if v38 == nil then
            break
        end
        if v39:IsA("Attachment") or v39.Name == "HasSnapped" then
            v39:Destroy()
        end
    end
end
