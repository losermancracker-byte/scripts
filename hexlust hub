local KeybindTab = Window:Tab({
    Title = "Misc",
    Icon = "bird", -- optional
    Locked = false,
})

local Keybind = KeybindTab:Keybind({
    Title = "Keybind",
    Desc = "Keybind to open ui",
    Value = "LeftControl",
    Callback = function(v)
        Window:SetToggleKey(Enum.KeyCode[v])
    end
})
